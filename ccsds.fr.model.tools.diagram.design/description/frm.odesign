                <subModelOperations xsi:type="tool_1:Unset" featureName="uses" elementExpression="[elementView.oclAsType(DEdge).targetNode.oclAsType(DNode).target/]"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
